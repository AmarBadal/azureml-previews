{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "EarlyTerminationSchema": {
            "type": "object",
            "properties": {},
            "additionalProperties": false
        },
        "SweepLimitsSchema": {
            "type": "object",
            "properties": {
                "max_concurrent_runs": {
                    "title": "max_concurrent_runs",
                    "type": "number",
                    "format": "integer"
                },
                "max_duration_minutes": {
                    "title": "max_duration_minutes",
                    "type": "number",
                    "format": "float"
                },
                "max_total_runs": {
                    "title": "max_total_runs",
                    "type": "number",
                    "format": "integer"
                }
            },
            "additionalProperties": false
        },
        "SweepObjectiveSchema": {
            "type": "object",
            "properties": {
                "goal": {
                    "title": "primary_metric_goal",
                    "type": "string"
                },
                "primary_metric": {
                    "title": "primary_metric_name",
                    "type": "string"
                }
            },
            "additionalProperties": false
        },
        "ChoiceSchema": {
            "type": "object",
            "properties": {
                "spec": {
                    "title": "spec",
                    "type": "string"
                },
                "values": {
                    "title": "values",
                    "type": "array",
                    "items": {
                        "anyOf": [
                            {
                                "title": "values",
                                "type": "string"
                            },
                            {
                                "title": "values",
                                "type": "number",
                                "format": "float"
                            }
                        ]
                    }
                }
            },
            "additionalProperties": false
        },
        "UniformSchema": {
            "type": "object",
            "properties": {
                "max_value": {
                    "title": "max_value",
                    "type": "number",
                    "format": "float"
                },
                "min_value": {
                    "title": "min_value",
                    "type": "number",
                    "format": "float"
                },
                "spec": {
                    "title": "spec",
                    "type": "string"
                }
            },
            "additionalProperties": false
        },
        "CommandJobSchema": {
            "type": "object",
            "properties": {
                "code": {
                    "anyOf": [
                        {
                            "type": "object",
                            "$ref": "#/definitions/CodeAssetSchema"
                        },
                        {
                            "type": "string",
                            "pattern": "^azureml:.*"
                        },
                        {
                            "type": "string",
                            "pattern": "^file:.*"
                        }
                    ]
                },
                "command": {
                    "title": "command",
                    "type": "string"
                },
                "compute": {
                    "type": "object",
                    "$ref": "#/definitions/ComputeBindingSchema"
                },
                "environment": {
                    "type": "string",
                    "pattern": "^azureml:.*"
                },
                "experiment_name": {
                    "title": "experiment_name",
                    "type": "string"
                },
                "inputs": {
                    "title": "inputs",
                    "type": "object",
                    "additionalProperties": {
                        "type": "object",
                        "$ref": "#/definitions/InputEntrySchema"
                    }
                },
                "job_type": {
                    "title": "job_type",
                    "type": "string"
                },
                "metadata": {
                    "type": "object",
                    "$ref": "#/definitions/JobMetadataSchema"
                },
                "name": {
                    "title": "name",
                    "type": "string"
                },
                "outputs": {
                    "title": "outputs",
                    "type": "object"
                },
                "properties": {
                    "title": "properties",
                    "type": "object"
                },
                "status": {
                    "title": "status",
                    "type": "string",
                    "readonly": true
                }
            },
            "additionalProperties": false
        },
        "CodeAssetSchema": {
            "type": "object",
            "properties": {
                "description": {
                    "title": "description",
                    "type": "string"
                },
                "directory": {
                    "title": "directory",
                    "type": "string"
                },
                "file": {
                    "title": "file",
                    "type": "string"
                },
                "id": {
                    "title": "id",
                    "type": "string"
                },
                "linkedServiceId": {
                    "title": "linked_service_id",
                    "type": "string"
                },
                "name": {
                    "title": "name",
                    "type": "string"
                },
                "tags": {
                    "title": "tags",
                    "type": "object"
                },
                "version": {
                    "title": "version",
                    "type": "number",
                    "format": "integer"
                }
            },
            "additionalProperties": false
        },
        "ComputeBindingSchema": {
            "type": "object",
            "properties": {
                "target": {
                    "type": "string",
                    "pattern": "^azureml:.*"
                },
                "node_count": {
                    "title": "node_count",
                    "type": "number",
                    "format": "integer"
                }
            },
            "additionalProperties": false
        },
        "InputEntrySchema": {
            "type": "object",
            "properties": {
                "mode": {
                    "title": "mode",
                    "type": "string",
                    "enum": [
                        "Mount",
                        "Download"
                    ],
                    "enumNames": []
                },
                "name": {
                    "type": "string",
                    "pattern": "^azureml:.*"
                }
            },
            "additionalProperties": false
        },
        "JobMetadataSchema": {
            "type": "object",
            "properties": {
                "endTimeUtc": {
                    "title": "endTimeUtc",
                    "type": "string",
                    "format": "date-time"
                },
                "interaction_endpoints": {
                    "type": "object",
                    "$ref": "#/definitions/InteractionEndpointsSchema"
                },
                "startTimeUtc": {
                    "title": "startTimeUtc",
                    "type": "string",
                    "format": "date-time"
                }
            },
            "additionalProperties": false
        },
        "InteractionEndpointsSchema": {
            "type": "object",
            "properties": {
                "grafana": {
                    "title": "grafana",
                    "type": "string"
                },
                "studio": {
                    "title": "studio",
                    "type": "string"
                },
                "tensorboard": {
                    "title": "tensorboard",
                    "type": "string"
                },
                "tracking": {
                    "title": "tracking",
                    "type": "string"
                }
            },
            "additionalProperties": false
        },
        "SweepJobSchema": {
            "type": "object",
            "properties": {
                "algorithm": {
                    "title": "algorithm",
                    "type": "string"
                },
                "early_termination": {
                    "type": "object",
                    "$ref": "#/definitions/EarlyTerminationSchema"
                },
                "experiment_name": {
                    "title": "experiment_name",
                    "type": "string"
                },
                "limits": {
                    "type": "object",
                    "$ref": "#/definitions/SweepLimitsSchema"
                },
                "name": {
                    "title": "name",
                    "type": "string"
                },
                "objective": {
                    "type": "object",
                    "$ref": "#/definitions/SweepObjectiveSchema"
                },
                "search_space": {
                    "title": "search_space",
                    "type": "object",
                    "additionalProperties": {
                        "anyOf": [
                            {
                                "type": "object",
                                "$ref": "#/definitions/ChoiceSchema"
                            },
                            {
                                "type": "object",
                                "$ref": "#/definitions/UniformSchema"
                            }
                        ]
                    }
                },
                "trial": {
                    "type": "object",
                    "$ref": "#/definitions/CommandJobSchema"
                }
            },
            "additionalProperties": false
        }
    },
    "$ref": "#/definitions/SweepJobSchema"
}