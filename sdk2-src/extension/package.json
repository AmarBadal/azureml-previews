{
	"name": "azml-yaml",
	"displayName": "AZML-YAML",
	"description": "config validation for azml yaml",
	"author": "Microsoft",
	"contributors": [
		{
			"name": "Brandon Horn",
			"email": "rihorn@microsoft.com"
		}
	],
	"license": "MIT",
	"version": "0.0.1",
	"publisher": "Microsoft",
    "preview": true,
    "engines": {
		"vscode": "^1.31.0"
	},
    "contributes": {
        "yamlValidation": [
            {
                "url": "https://azuremlsdk2.blob.core.windows.net/latest/commandJob.schema.json",
                "fileMatch": ["*-CommandJob.yaml", "*-CommandJob.yml"]
            }, {
                "url": "https://azuremlsdk2.blob.core.windows.net/latest/sweepJob.schema.json",
                "fileMatch": ["*-SweepJob.yaml", "*-SweepJob.yml"]
            }, {
                "url": "https://azuremlsdk2.blob.core.windows.net/latest/schema/asset.schema.json",
                "fileMatch": ["*-Asset.yaml", "*-Asset.yml"]
            }, {
                "url": "https://azuremlsdk2.blob.core.windows.net/latest/schema/codeAsset.schema.json",
                "fileMatch": ["*-Code.yaml", "*-Code.yml"]
            }, {
                "url": "https://azuremlsdk2.blob.core.windows.net/latest/schema/onlineEndpoint.schema.json",
                "fileMatch": ["*-OnlineEndpoint.yaml", "*-OnlineEndpoint.yml"]
            }, {
                "url": "https://azuremlsdk2.blob.core.windows.net/latest/schema/endpointDeployment.schema.json",
                "fileMatch": ["*-Deployment.yaml", "*-Deployment.yml"]
            }, {
                "url": "https://azuremlsdk2.blob.core.windows.net/latest/schema/environment.schema.json",
                "fileMatch": ["*-Environment.yaml", "*-Environment.yml"]
            }, {
                "url": "https://azuremlsdk2.blob.core.windows.net/latest/schema/model.schema.json",
                "fileMatch": ["*-Model.yaml", "*-Model.yml"]
            }
        ]
    }
}
