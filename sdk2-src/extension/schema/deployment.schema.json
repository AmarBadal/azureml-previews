{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "CodeConfigurationSchema": {
            "type": "object",
            "properties": {
                "code": {
                    "anyOf": [
                        {
                            "type": "string",
                            "pattern": "^azureml:.*"
                        },
                        {
                            "type": "object",
                            "$ref": "#/definitions/CodeAssetSchema"
                        },
                        {
                            "type": "string",
                            "pattern": "^file:.*"
                        }
                    ]
                },
                "scoring_script": {
                    "title": "scoring_script",
                    "type": "string"
                }
            },
            "additionalProperties": false
        },
        "CodeAssetSchema": {
            "type": "object",
            "properties": {
                "description": {
                    "title": "description",
                    "type": "string"
                },
                "directory": {
                    "title": "directory",
                    "type": "string"
                },
                "file": {
                    "title": "file",
                    "type": "string"
                },
                "id": {
                    "title": "id",
                    "type": "string"
                },
                "linkedServiceId": {
                    "title": "linked_service_id",
                    "type": "string"
                },
                "name": {
                    "title": "name",
                    "type": "string"
                },
                "tags": {
                    "title": "tags",
                    "type": "object"
                },
                "version": {
                    "title": "version",
                    "type": "number",
                    "format": "integer"
                }
            },
            "additionalProperties": false
        },
        "EnvironmentSchema": {
            "type": "object",
            "properties": {
                "docker": {
                    "title": "docker",
                    "type": "object"
                },
                "name": {
                    "title": "name",
                    "type": "string"
                },
                "path": {
                    "title": "path",
                    "type": "string"
                },
                "python": {
                    "title": "python",
                    "type": "object"
                },
                "runtime": {
                    "title": "runtime",
                    "type": "string"
                },
                "version": {
                    "title": "version",
                    "type": "number",
                    "format": "integer"
                }
            },
            "additionalProperties": false
        },
        "ModelSchema": {
            "type": "object",
            "properties": {
                "asset_path": {
                    "title": "asset_path",
                    "type": "string"
                },
                "description": {
                    "title": "description",
                    "type": "string"
                },
                "environment": {
                    "type": "string",
                    "pattern": "^azureml:.*"
                },
                "flavors": {
                    "title": "flavors",
                    "type": "object"
                },
                "input_example": {
                    "title": "input_example",
                    "type": "object"
                },
                "name": {
                    "title": "name",
                    "type": "string"
                },
                "tags": {
                    "title": "tags",
                    "type": "object"
                },
                "utc_time_created": {
                    "title": "utc_time_created",
                    "type": "string",
                    "format": "date-time"
                },
                "version": {
                    "title": "version",
                    "type": "number",
                    "format": "integer"
                }
            },
            "required": [
                "asset_path",
                "name",
                "version"
            ],
            "additionalProperties": false
        },
        "RequestSettingsSchema": {
            "type": "object",
            "properties": {
                "max_concurrent_requests_per_instance": {
                    "title": "max_concurrent_requests_per_instance",
                    "type": "number",
                    "format": "integer"
                },
                "max_queue_wait_ms": {
                    "title": "max_queue_wait_ms",
                    "type": "number",
                    "format": "integer"
                },
                "request_timeout_ms": {
                    "title": "request_timeout_ms",
                    "type": "number",
                    "format": "integer"
                }
            },
            "additionalProperties": false
        },
        "ResourceRequirementsSchema": {
            "type": "object",
            "properties": {
                "cpu": {
                    "title": "cpu",
                    "type": "number",
                    "format": "float"
                },
                "gpu": {
                    "title": "gpu",
                    "type": "number",
                    "format": "integer"
                },
                "memory": {
                    "title": "memory",
                    "type": "number",
                    "format": "float"
                }
            },
            "additionalProperties": false
        },
        "ScaleSettingsSchema": {
            "type": "object",
            "properties": {
                "instance_count": {
                    "title": "instance_count",
                    "type": "number",
                    "format": "integer"
                },
                "max_instances": {
                    "title": "max_instances",
                    "type": "number",
                    "format": "integer"
                },
                "min_instances": {
                    "title": "min_instances",
                    "type": "number",
                    "format": "integer"
                },
                "scale_type": {
                    "title": "scale_type",
                    "type": "string"
                }
            },
            "additionalProperties": false
        },
        "OnlineEndpointDeploymentSchema": {
            "type": "object",
            "properties": {
                "code_configuration": {
                    "type": "object",
                    "$ref": "#/definitions/CodeConfigurationSchema"
                },
                "environment": {
                    "anyOf": [
                        {
                            "type": "string",
                            "pattern": "^azureml:.*"
                        },
                        {
                            "type": "object",
                            "$ref": "#/definitions/EnvironmentSchema"
                        }
                    ]
                },
                "id": {
                    "title": "id",
                    "type": "string",
                    "readonly": true
                },
                "model": {
                    "anyOf": [
                        {
                            "type": "string",
                            "pattern": "^azureml:.*"
                        },
                        {
                            "type": "object",
                            "$ref": "#/definitions/ModelSchema"
                        }
                    ]
                },
                "name": {
                    "title": "name",
                    "type": "string",
                    "readonly": true
                },
                "provisioning_status": {
                    "title": "provisioning_status",
                    "type": "string",
                    "readonly": true
                },
                "request_settings": {
                    "type": "object",
                    "$ref": "#/definitions/RequestSettingsSchema"
                },
                "resource_requirements": {
                    "type": "object",
                    "$ref": "#/definitions/ResourceRequirementsSchema"
                },
                "scale_settings": {
                    "type": "object",
                    "$ref": "#/definitions/ScaleSettingsSchema"
                },
                "sku": {
                    "title": "sku",
                    "type": "string"
                },
                "tags": {
                    "title": "tags",
                    "type": "object"
                },
                "type": {
                    "title": "type",
                    "type": "string",
                    "readonly": true
                }
            },
            "additionalProperties": false
        }
    },
    "$ref": "#/definitions/OnlineEndpointDeploymentSchema"
}